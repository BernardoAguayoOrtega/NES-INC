{
	"name": "frontend",
	"version": "1.0.0",
	"description": "Frontend of news inc project by ...",
	"main": "index.js",
	"scripts": {
		"build": "webpack --mode production",
		"dev": "webpack-dev-server",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Team-C5-cheaPlatzi/Frontend.git"
	},
	"keywords": [
		"react.js",
		"frontend",
		"js",
		"enzyme",
		"jest",
		"husky"
	],
	"author": "Team 5",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Team-C5-cheaPlatzi/Frontend/issues"
	},
	"homepage": "https://github.com/Team-C5-cheaPlatzi/Frontend#readme",
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.1",
		"babel-jest": "^26.5.2",
		"babel-loader": "^8.1.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"css-loader": "^5.0.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"enzyme-to-json": "^3.6.1",
		"eslint": "^7.11.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.21.4",
		"eslint-plugin-react-hooks": "^4.1.2",
		"file-loader": "^6.1.1",
		"html-webpack-plugin": "^4.5.0",
		"husky": "^4.3.0",
		"jest": "^26.5.3",
		"jest-css-modules": "^2.1.0",
		"jsdom": "^16.4.0",
		"style-loader": "^2.0.0",
		"stylus-loader": "^4.1.1",
		"url-loader": "^4.1.1",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"disqus-react": "^1.0.10",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-router-dom": "^5.2.0",
		"stylus": "^0.54.8"
	},
	"jest": {
		"verbose": true,
		"setupFilesAfterEnv": [
			"<rootDir>./setupTest.js"
		],
		"moduleNameMapper": {
			"\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules",
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/./src/__mocks__/fileMock.js"
		},
		"transform": {
			"^.+\\.[t|j]sx?$": "babel-jest"
		},
		"moduleFileExtensions": [
			"js",
			"json",
			"jsx",
			".test.(js)$/",
			"node"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run test"
		}
	}
}
