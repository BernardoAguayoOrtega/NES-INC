@import '../../globalStyles.styl'

.card
  display: grid
  grid-template-columns: 1fr
  grid-template-rows: 1fr
  font-family: Font1
  &__image
    z-index: 2
    width: 364px
    height: 400px
    background-color: darkGray
  &__title
    z-index: 3
    bottom: 200px
    font-size: 2.3rem
    font-weight: 800
    color: lightGray